{
    "functions": {
        "api\/*.php": {
            "runtime": "vercel-php@0.7.3"
        }
    },
    "routes": [
        {
            "src": "\/(assets)\/(.*)$",
            "dest": "\/api\/assets.php?path=\/$2"
        },
        {
            "src": "\/(bundles)\/(.*)$",
            "dest": "\/api\/assets.php?path=\/$2"
        },
        {
            "src": "\/(.*)",
            "dest": "\/api\/index.php"
        }
    ]
}